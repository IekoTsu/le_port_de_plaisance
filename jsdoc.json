{
  "opts": {
    "destination": "./docs",  
    "recurse": true,
    "template": "node_modules/docdash",
    "readme": "README.md"
  },
  "source": {
    "include": [
      "./"  
    ],
    "includePattern": ".+\\.js(doc|x)?$",  
    "excludePattern": "(node_modules|\\.git)" 
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "docdash": {
    "sort":true,
    "search":true,
    "collapse":false
  }
}
