<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Liste des catways</title>
</head>
<body>
    <h1>Catways</h1>
    <div>
        <% const msg = typeof message !== 'undefined' ? message : '' %>
        <% if (msg) {  %>
            <p> <%- msg %> </p>
        <% } %>
    </div>
    <a href="/catways/add">Créer un nouveau Catway</a>
    <ul>
        <% catways.forEach(catway => { %>
            <li>
                <a href="/catways/<%= catway._id %>"><%= catway.boatName %> - <%= catway.catwayState %></a> |
                <a href="/catways/<%= catway._id %>/edit">Modifier</a>
            </li>
        <% }); %>
    </ul>
    <a href="/dashboard">Accéder au tableau de bord</a>
</body>
</html>
